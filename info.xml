<?xml version="1.0"?>
<extension key="com.joineryhq.textselect" type="module">
  <file>textselect</file>
  <name>TextSelect</name>
  <description>Converts some text fields to select fields; currently only supports Contribution Source field.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.textselect</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.textselect</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.textselect/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-20</releaseDate>
  <version>2.0.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Textselect</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Textselect_Upgrader</upgrader>
</extension>
